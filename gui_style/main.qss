/*字体颜色*/
QWidget{
    color: rgb(206,206,206);
}
/*header,booter*/
QFrame#header,QFrame#booter{
    background-color: rgb(60,63,65);
}
QFrame#body{
    background-color: rgb(43,43,43);
}
/*最小化，最大化，关闭*/
QPushButton#min_btn,QPushButton#max_btn,QPushButton#close_btn{
    border:none;
}
QPushButton#min_btn:pressed,QPushButton#max_btn:pressed,QPushButton#close_btn:pressed{
    background-color: rgb(255,237,252);
    border-top:5px;
    border-left:5px;
}
/*关闭*/
QPushButton#close_btn:hover {
    background-color: rgb(255, 0, 0);
}
/*最小化,最大化*/
QPushButton#min_btn:hover,QPushButton#max_btn:hover {
	background-color: rgba(160, 160, 160, 80);
}
/*进度条*/
QProgressBar#progressBar{
    min-height: 4px;
    max-height: 4px;
    border-radius: 2px;
    background-color: rgb(60,63,65);
}
QProgressBar#progressBar::chunk {
    border-radius: 2px;
    background-color: #009688;
}
/* body_left */
QFrame#body_left{
    border-right: 1px solid rgba(204, 204, 204, 30);
    background-color: rgba(204, 204, 204, 30);
}
/*按钮 */
QPushButton#search:hover,QPushButton#add_btn:hover,QPushButton#exit_btn:hover,QPushButton#setting_btn:hover,QPushButton#manage_btn:hover,QPushButton#extend_btn:hover,QPushButton#about_btn:hover {
	background-color: rgba(204, 204, 204, 60);
	border-radius:10px;
}
QPushButton#path_btn:pressed,QPushButton#log_btn:pressed,QPushButton#public_btn:pressed,QPushButton#private_btn:pressed,QPushButton#db_bth:pressed,QPushButton#search:pressed,QPushButton#add_btn:pressed,QPushButton#exit_btn:pressed,QPushButton#setting_btn:pressed,QPushButton#manage_btn:pressed,QPushButton#extend_btn:pressed,QPushButton#about_btn:pressed {
    background-color: rgba(85, 255, 255, 60);
    border-top:5px;
    border-left:5px;
    border-radius:10px;
}
/*db_bth path_btn log_btn public_btn private_btn */
QPushButton#db_bth, QPushButton#path_btn,QPushButton#log_btn,QPushButton#public_btn,QPushButton#private_btn{
	background-image: url(:/color/images/color/open_dir.svg);
	background-repeat: repeat-no-repeat;
	background-position: left;
	background-origin:content;
	text-align : right;
	border-radius:10px;
}
/* search */
