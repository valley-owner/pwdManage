	background-image: url(:/blue/images/blue/sousuo-xianxing.svg);
	background-repeat: repeat-no-repeat;
	background-position: left;
	background-origin:content;
	text-align :right;
	padding-right:10px;
	border:1px solid rgba(204, 204, 204, 150);
	border-radius:10px;
	padding-left:4px;
}
/*del_btn*/
QPushButton#del_btn{
