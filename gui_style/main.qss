/*下来选择框*/

QComboBox{
    color: rgba(255, 255, 255, 200);
    font-size:14px;
    padding: 1px 15px 1px 10px;
    border:1px solid rgba(204, 204, 204, 150);
    border-radius: 12px; /* 边框圆角 */
    background-color: rgb(43,43,43);
}
QComboBox::drop-down {
    subcontrol-origin: padding;
    subcontrol-position: top right;
    width: 15px;
    border:none;
}
QComboBox::down-arrow {
    image: url(:/blue/images/blue/xiala.svg);
	padding-right:18px;
}
QComboBox QAbstractItemView{
	background:rgba(255,255,255,1);
    border:1px solid rgba(228,228,228,1);
    border-radius:0px 0px 5px 5px;
	font-size:14px;
    outline: 0px;  /*去虚线*/
}

QComboBox QAbstractItemView::item{
	height:36px;
	color:#666666;
	padding-left:9px;
	background-color:#FFFFFF;
}
QComboBox QAbstractItemView::item:hover{ /*悬浮*/
    background-color:#409CE1;
    color:#ffffff;
}
QComboBox QAbstractItemView::item:selected{  /*选中*/
    background-color:#409CE1;
    color:#ffffff;
}

QComboBox:on {
    padding-top: 3px;
    padding-left: 10px;
}
QComboBox::down-arrow:on {
    top: 1px;
    left: 1px;
}
/*输入框*/
QLineEdit#keyword{
	color: rgba(255,255,255,200);
	border:1px solid rgba(204,204,204,150);
	outline: none;
	border-radius: 12px; /* 边框圆角 */
	padding-left: 12px; /* 文本距离左边界有5px */
	background-color: rgb(43,43,43);
}
QLineEdit#keyword:focus{
	/* border: 1px solid rgb(0, 255, 255); */
	border-radius: 12px; /* 边框圆角 */
	background-color: rgb(43,43,43);
}
QLineEdit#keyword[echoMode="2"] { /* QLineEdit有输入掩码时的状态 */
	lineedit-password-character: 9679;
	lineedit-password-mask-delay: 2000;
	background-color: rgb(43,43,43);
}
/*tab*/
QHeaderView{
	color: rgb(255,0,127);
	background-color: rgb(43,43,43);
	color: rgb(255,255,255);
}
QHeaderView::section{
    font-size:16px;
    font-family:"微软雅黑";
    color:#FFFFFF;
    background-color: rgb(43,43,43);
    border:none;
	border-right: 1px solid rgba(204,204,204,150);
	border-bottom: 1px solid rgba(204,204,204,150);
    text-align:center;
    min-height:49px;
    max-height:49px;
    margin-left:0px;
    padding-left:0px;
}
QTableView QTableCornerButton::section {
    font-size:16px;
    font-family:"微软雅黑";
    color:#FFFFFF;
    background-color: rgb(43,43,43);
	border: none;
    text-align:center;
    min-height:49px;
    max-height:49px;
    margin-left:0px;
    padding-left:0px;
 }
QTableWidget{
	border:none;
	background-color: rgb(43, 43, 43);
    font-size:14px;
    font-family:"微软雅黑";
	text-align:center;
	color: rgb(255, 255, 255);
}
QTableWidget::item{
    border: none ;
	border-bottom: 1px solid rgba(204,204,204,150);
	border-right: 1px solid rgba(204,204,204,150);
	text-align:center;
}
QTableWidget::item:selected{
	color: #ff007f;
	font-size:16px;
}
QScrollBar::handle:vertical{
    background: rgba(255,255,255,20%);
    border: 0px solid grey;
    border-radius:3px;
    width: 8px;
}
QScrollBar:vertical{
    border-width:1px;
    border-style: solid;
    border-color: rgba(255,255,255,10%);
    width: 8px;
    margin:0px 0px 0px 0px;
    border-radius:3px;
}
QScrollBar::add-page:vertical, QScrollBar::sub-page:vertical{
    background:rgba(255,255,255,10%);
}
QScollBar::add-line:vertical , QScrollBar::sub-line:vertical{
    background:transparent;
}
QScrollBar::handle:horizontal{
    background: rgba(255,255,255,20%);
    border: 0px solid grey;
    border-radius:3px;
    width: 8px;
}
QScrollBar:horizontal{
    border-width:1px;
    border-style: solid;
    border-color: rgba(255,255,255,10%);
	height:8px;
    margin:0px 0px 0px 0px;
    border-radius:3px;
}
QScrollBar::add-page:horizontal, QScrollBar::sub-page:horizontal{
    background:rgba(255,255,255,10%);
}
QScollBar::add-line:horizontal, QScrollBar::sub-line:horizontal{
    background:transparent;
}
/*配置文件 db_label path_label log_label pub_key_label priv_key_label*/
QLabel#db_label,QLabel#path_label,QLabel#log_label,QLabel#pub_key_label,QLabel#priv_key_label{
    border-bottom:1px solid rgba(204,204,204,150);
}
/*开发初中*/
QLabel#label_7,QLabel#label_8,QLabel#label_11{
    color:rgb(0,85,255);
    background-color:rgb(43,43,43);
}
/*密钥保管*/
QLabel#label_13{
    color:rgb(255,85,0);
}
QScrollArea{
    border:none;
    background-color: rgb(43,43,43);
}
QTextBrowser{
    border:none;
    background-color:rgb(43,43,43);
}
QScrollBar:vertical{
    width:8px;
    background:transparent;
    margin:0px 0px 0px 0px;
    padding-top:0px;
    padding-bottom:0px;
}
QScrollBar::handle:vertical{
    width:8px;
    background:rgba(85,255,255,180);
    border-radius:4px;
    min-height:30px;
}
QScrollBar::handle:vertical:hover{
    width:8px;
    background:rgba(85,255,255,180);
    border-radius:4px;
    min-height:30;
}
QScrollBar::add-line:vertical{
    height:0px;
    width:8px;
}
QScrollBar::sub-line:vertical{
    height:0px;
    width:8px;
    subcontrol-position:top;
}
QScrollBar::add-page:vertical,QScrollBar::sub-page:vertical{
    background:transparent;
    border-radius:4px;
}
QStackedWidget#stacked_page{
    background-color: rgb(43,43,43);
}